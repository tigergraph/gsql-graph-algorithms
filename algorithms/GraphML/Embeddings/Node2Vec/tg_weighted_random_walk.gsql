CREATE QUERY tg_weighted_random_walk(INT walk_length = 20, INT num_walks = 4, FLOAT p = 1, FLOAT q = 1, STRING filepath = "/home/tigergraph/path.csv")  SYNTAX V1 { 
    
    FILE f (filepath);
    Start = {ANY};
    Start = SELECT s
            FROM Start:s
            POST-ACCUM tg_weighted_random_walk_sub(s, walk_length, num_walks, p, q, f);
            
}