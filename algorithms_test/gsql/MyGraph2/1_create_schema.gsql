# 1. Create graph
CREATE GRAPH MyGraph2 ()

# 2. Create schema_change job
CREATE SCHEMA_CHANGE JOB change_schema_of_MyGraph FOR GRAPH MyGraph2 {
  # 2.1 Create vertices
  ADD VERTEX MyNode (PRIMARY_ID id STRING, community STRING) WITH PRIMARY_ID_AS_ATTRIBUTE="true";

  # 2.2 Create edges
  ADD DIRECTED EDGE MyEdge (FROM MyNode, TO MyNode, weight DOUBLE) WITH REVERSE_EDGE="rev_MyEdge";
}

# 3. Run schema_change job
RUN SCHEMA_CHANGE JOB change_schema_of_MyGraph

# 4. Drop schema_change job
DROP JOB change_schema_of_MyGraph
